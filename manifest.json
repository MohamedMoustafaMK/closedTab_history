{
  "manifest_version": 3,
  "name": "Tab Saver",
  "version": "1.0",
  "description": "Save and store open tab URLs",
  "permissions": ["tabs"],
  "action": {
    "default_icon": "cross.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "cross.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
